<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="us.hopecoders.catchy_care_carpentry">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CatchyCareCarpentry">
        <activity android:name=".intros.OnBoardingActivity"

            >

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".auth.ConfirmationCode" />
        <activity android:name=".MainActivity"/>
        <activity android:name=".auth.SignUp"/>
        <activity android:name=".auth.SignIn"/>

    </application>

</manifest>